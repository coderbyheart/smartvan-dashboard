{
  "name": "@coderbyheart/smartvan-dashboard",
  "version": "0.0.0-development",
  "description": "Web app for the SmartVan AWS serverless backend",
  "scripts": {
    "start": "snowpack dev --polyfill-node",
    "build": "snowpack build --polyfill-node",
    "test": "echo \"Error: no test specified\" && exit 1",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coderbyheart/smartvan-dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/coderbyheart/smartvan-dashboard/issues"
  },
  "homepage": "https://github.com/coderbyheart/smartvan-dashboard",
  "keywords": [
    "Campervan",
    "IoT",
    "AWS",
    "serverless"
  ],
  "author": "Markus Tacker | https://coderbyheart.com",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-timestream-query": "^1.0.0-rc.8",
    "@bifravst/timestream-helpers": "1.3.1",
    "amazon-cognito-identity-js": "^4.5.5",
    "aws-amplify": "^3.3.11",
    "aws-amplify-react": "^4.2.15",
    "bootstrap": "4.5.3",
    "chart.js": "^2.9.4",
    "date-fns": "^2.16.1",
    "minimal.css": "^1.0.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "reactstrap": "8.7.1",
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "@bifravst/code-style": "8.0.134",
    "@bifravst/eslint-config-typescript": "5.0.81",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-angular": "11.0.0",
    "@prefresh/snowpack": "2.1.0",
    "@snowpack/plugin-babel": "2.1.5",
    "@snowpack/plugin-react-refresh": "2.3.8",
    "@snowpack/plugin-typescript": "1.1.1",
    "@types/chart.js": "^2.9.28",
    "@types/classnames": "2.2.11",
    "@types/jest": "26.0.18",
    "@types/node": "14.14.11",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/reactstrap": "8.7.2",
    "@types/snowpack-env": "2.3.2",
    "@types/styled-components": "5.1.4",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "babel-eslint": "10.1.0",
    "eslint": "7.15.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-app": "6.2.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.6",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "snowpack": "2.18.2",
    "snyk": "1.434.3",
    "typescript": "4.1.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts,.jsx,.tsx"
    ],
    "*.{md,json,yaml,yml,html}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "browserslist": [
    ">3%",
    "last 1 version",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "snyk": true
}
